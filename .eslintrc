{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["react", "jsx-a11y", "import", "prettier"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "prettier/prettier": "error"
  },
  "prettier/prettier": [
    "error",
    {
      "trailingComma": "es5",
      "singleQuote": true,
      "printWidth": 120
    }
  ]
}